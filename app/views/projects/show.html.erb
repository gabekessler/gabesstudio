<p>
  <b>Title:</b>
  <%=h @project.title %>
</p>

<p>
  <b>Link:</b>
  <%=h @project.link %>
</p>

<p>
  <b>Type:</b>
  <%=h @project.project_type %>
</p>

<p>
  <b>Text:</b>
  <%=h @project.text %>
</p>

<p>
  <b>Completion date:</b>
  <%=h @project.completion_date.strftime("%m/%d/%y") %>
</p>

<h2>Gallery Images</h2>
    <% for gallery_image in @gallery_images %>
        <%= image_tag gallery_image.image.url(:long), :class => "long_image" %>
    <% end %>

<br class="clear" />


<%= link_to 'Edit', edit_project_path(@project) %> |
<%= link_to 'Back', projects_path %>
<br />
<%= link_to 'Images', project_gallery_images_path(@project) %>