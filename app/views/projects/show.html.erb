<div id="middle_column" class="span-4">
  <ul id="tag_list">
  <% tag_cloud @tags, %w(css1 css2 css3 css4) do |tag, css_class| %>
    <li><%= link_to tag.name, { :action => :tag, :id => tag.name }, :class=>((@project.tag_list.include?(tag.name)) ? 'project_tag': css_class)  %></li>
  <% end %>
  </ul>
</div><!--middle_column--><div class="span-14 last">
  
<h1><%=h @project.title %></h1>

<p>
<%= link_to "#{@project.link}", "http://#{@project.link}", :class => "ss_sprite ss_link_go" %>
</p>

<p>
  <%= @project.text %>
</p>

<p>
  <strong>Completion date:</strong>
  <%=h @project.completion_date.strftime("%m/%d/%y") %>
</p>

<div id="gallery_area">
<h2>Gallery Images</h2>
    <% for gallery_image in @gallery_images %>
        <%= link_to image_tag(gallery_image.image.url(:long)), gallery_image_path(gallery_image) %>
    <% end %>
<br class="clear" />
</div><!--gallery_area-->


<%= link_to 'Edit', edit_project_path(@project) %> |
<%= link_to 'Back', projects_path %>
<br />
<%= link_to 'Images', project_gallery_images_path(@project) %>

</div><!--span-14 last-->